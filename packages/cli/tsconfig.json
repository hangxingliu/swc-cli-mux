{
    "compilerOptions": {
        "target": "es2019",
        "module": "commonjs",
        "noEmit": true,
        "strict": true,
        "strictNullChecks": false,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "lib": ["ES2019.Array", "ES2020.Promise", "ES2015"]
    },
    "exclude": ["**/__tests__/**"],
    "include": ["src"]
}
