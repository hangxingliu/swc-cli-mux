{
    "name": "swc-cli-mux",
    "version": "0.7.3",
    "description": "CLI for the swc project",
    "main": "lib/swc/index.js",
    "scripts": {
        "prepublishOnly": "yarn build && cp ../../README.md README.md",
        "prepack": "cp ../../README.md README.md",
        "types": "tsc",
        "types:watch": "tsc --watch",
        "build": "tsc",
        "build:watch": "tsc --watch",
        "format:src": "prettier --write src",
        "format:examples": "prettier --write examples",
        "format": "yarn format:src && yarn format:examples",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "JEST_COVERAGE=true jest --coverage",
        "tgz": "yarn pack --out '%s-%v.tgz'"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/hangxingliu/swc-cli-mux"
    },
    "keywords": [
        "swc",
        "cli",
        "babel",
        "es6",
        "transpile",
        "transpiler",
        "compiler",
        "javascript"
    ],
    "author": "강동윤 <kdy1997.dev@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/hangxingliu/swc-cli-mux/issues"
    },
    "homepage": "https://github.com/hangxingliu/swc-cli-mux",
    "engines": {
        "node": ">= 16.14.0"
    },
    "bin": {
        "spack": "./bin/spack.js",
        "swc": "./bin/swc.js",
        "swc-mux": "./bin/swc-mux.js",
        "swcx": "./bin/swcx.js"
    },
    "dependencies": {
        "@xhmikosr/bin-wrapper": "^13.0.5",
        "commander": "^8.3.0",
        "fast-glob": "^3.3.3",
        "minimatch": "^9.0.5",
        "piscina": "^4.9.2",
        "semver": "^7.7.1",
        "slash": "3.0.0",
        "source-map": "^0.7.4"
    },
    "devDependencies": {
        "@swc/cli": "^0.7.3",
        "@swc/core": "^1.11.24",
        "@swc/jest": "^0.2.38",
        "@swc/types": "^0.1.21",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.15.3",
        "@types/semver": "^7.7.0",
        "chokidar": "^4.0.1",
        "deepmerge": "^4.3.1",
        "jest": "^29.7.0",
        "ts-jest": "^29.3.2",
        "typescript": "^5.8.3"
    },
    "peerDependencies": {
        "@swc/core": "^1.11",
        "chokidar": "^4"
    },
    "peerDependenciesMeta": {
        "chokidar": {
            "optional": true
        }
    },
    "files": [
        "bin",
        "lib",
        "src",
        "!src/**/__tests__",
        "!src/**/__mocks__"
    ],
    "type": "commonjs",
    "types": "src/swc/index.ts",
    "exports": {
        "./src/*.js": {
            "require": "./lib/*.js",
            "types": "./src/*.ts"
        },
        ".": {
            "require": "./lib/swc/index.js",
            "types": "./src/swc/index.ts"
        }
    },
    "maintainers": [
        "Liu Yue <hangxingliu@gmail.com>"
    ]
}